package clases;

import javax.swing.JFrame;

public class Formulario extends JFrame{

	public Formulario(){
		setTitle("Figuras");
		setSize(500,500);
		setLocationRelativeTo(null);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		setVisible(true);
	}
	public static void main(String[] args) {
		new Formulario();
	}

}
